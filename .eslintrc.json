{
    "env": {
        "browser": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "off",
        "no-unused-vars": "warn",
        "no-use-before-define": "off",
        "no-plusplus": "off",
        "no-loop-func": "off",
        "no-lonely-if": "off",
        "no-param-reassign": "off"
    }
}
